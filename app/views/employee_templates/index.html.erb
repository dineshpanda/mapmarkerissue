<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All employees
    </h1>

    
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Name
        </th>

        <th>
          Emp
        </th>

        <th>
          Emp type
        </th>

        <th>
          Photo
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @employees.each do |employee| %>
      <tr>
        <!-- Display employee.id start -->
        <td>
          <%= employee.id %>
        </td>
        <!-- Display employee.id end -->

        <!-- Display employee.name start -->
        <td>
          <%= employee.name %>
        </td>
        <!-- Display employee.name end -->

        <!-- Display employee.emp_id start -->
        <td>
          <%= employee.emp_id %>
        </td>
        <!-- Display employee.emp_id end -->

        <!-- Display employee.emp_type start -->
        <td>
          <%= employee.emp_type %>
        </td>
        <!-- Display employee.emp_type end -->

        <!-- Display employee.photo start -->
        <td>
          <%= employee.photo %>
        </td>
        <!-- Display employee.photo end -->

        <!-- Display employee.created_at start -->
        <td>
          <%= time_ago_in_words(employee.created_at) %> ago
        </td>
        <!-- Display employee.created_at end -->

        <!-- Display employee.updated_at start -->
        <td>
          <%= time_ago_in_words(employee.updated_at) %> ago
        </td>
        <!-- Display employee.updated_at end -->

        <td>
          <a href="/employees/<%= employee.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
